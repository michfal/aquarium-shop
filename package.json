{
  "name": "aqua-shop",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "seed": "tsx --tsconfig tsconfig.scripts.json scripts/seed.ts",
    "seed:reset": "tsx --tsconfig tsconfig.scripts.json scripts/seed.ts --reset"
  },
  "dependencies": {
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@supabase/supabase-js": "^2.56.0",
    "@tanstack/vue-query": "^5.87.4",
    "dotenv": "^17.2.1",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@tsconfig/node22": "^22.0.2",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^22.18.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.5",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4"
  }
}
